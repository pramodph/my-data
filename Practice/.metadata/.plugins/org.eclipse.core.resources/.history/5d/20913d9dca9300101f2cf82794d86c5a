package slidingwindow;

public class OptimisedBinarySubArrayWithSum {

	public static void main(String[] args) {
			int[] nums= {0,0,0,0,0};
			int k=0;
			System.out.println(numSubArrayWithSum(nums,k));
	}

	private static char[] numSubArrayWithSum(int[] nums, int k) {
		int end=0;
		int start=0;
		int n=nums.length;
		int count=0;
		while(start<end) {
			int sum=0;
			sum=sum+nums[end];
			if(sum==k) {
				count++;
			}
			while(sum>k) {
				sum=sum-nums[start];
				if(sum==k) {
					count++;
				}
				start++;
			}
			end++;
		}
		return count;
	}

}
