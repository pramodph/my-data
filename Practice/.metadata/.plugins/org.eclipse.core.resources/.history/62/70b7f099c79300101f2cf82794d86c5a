package slidingwindow;

public class BinarySubArrayWithSum {

	public static void main(String[] args) {
		int[] nums= {1,0,1,0,1};
		int k=2;
		System.out.println(numSubArrayWithSum(nums,k));
	}
	public  static int numSubArrayWithSum(int[] nums,int k) {
		int n=nums.length;
		int count=0;
		int sum=0;
		for(int i=0;i<n;i++) {
			for(int j=i;j<n;j++) {
				sum+=nums[i];
				if(sum==k) {
					count++;
				}
			}
		}
		return count;
		
	}

}
