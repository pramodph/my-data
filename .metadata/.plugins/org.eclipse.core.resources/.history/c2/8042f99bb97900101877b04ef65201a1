package multiThreadingDemos;

class Display1{
	Display d;
	String name;
	
	public Display1(Display d, String name) {
		this.d = d;
		this.name = name;
	}

	public void display2() {
		for(int i=0;i<=10;i++) {
			System.out.println(i);
		}
	}
}

class ExThread03 extends Thread{
	Display d;
	String name;
	public ExThread03(Display d, String name) {
		this.d = d;
		this.name = name;
	}
	
	public void run() {
		d.display1();
	}
}

class ExThread04 extends Thread{
	Display d;
	String name;
	public ExThread02(Display d, String name) {
		this.d = d;
		this.name = name;
	}
	
	public void run() {
		d.display2();
	}
}
public class Demo07 {
	Display1 d1=new Display1();
	ExThread03 t3=new ExThread03();
	ExThread04 t4=new ExThread04();
	

}
