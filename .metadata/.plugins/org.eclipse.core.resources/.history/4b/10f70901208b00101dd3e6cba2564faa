package gsquestions;

public class JoshephusElimination {

	public static void main(String[] args) {
		int n=1;
		int k=1;
		System.out.println(findTheWinner(n,k));
		
	}

	private static int findTheWinner(int n, int k) {
		if(n<1) return 0;
		int winner=0;
		for(int i=2;i<=n;i++) {
			winner=(winner+k)%i;
		}
		return winner+1;
	}
//Time complexity is O(n) and space complexity is O(1)
//list edge cases 
}
