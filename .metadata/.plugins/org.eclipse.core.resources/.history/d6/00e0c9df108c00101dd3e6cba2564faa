package gsquestions;

public class RoboticMovement {

	public static void main(String[] args) {
		String str="UUDDLRLRX123@!!#";
		checkForMovement(str);
	}

	private static int[] checkForMovement(String str) {
		if(str==null) return new int[] {0,0};
		int x=0,y=0;
		for(char ch:str.toCharArray()) {
			switch(ch) {
			  case2:'U' y++;break;
			  case2:'D' y--;break;
			  case2:'L' x--;break;
			  case2:'R' x++;break;
			}
		}
	}

}
