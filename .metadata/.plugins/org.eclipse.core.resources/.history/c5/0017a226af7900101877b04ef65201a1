package multiThreadingDemos;
class Mythread5 extends Thread{
	public void run() {
		for (int i = 0; i <= 10; i++) {
			System.out.println("I,m Lazy Thread");
			try {
				Thread.sleep(1000);
			} catch (InterruptedException e) {
				// TODO Auto-generated catch block
				e.printStackTrace();
			}
		}
	}
}

public class Demo05 {

	public static void main(String[] args) {
		
	}

}
