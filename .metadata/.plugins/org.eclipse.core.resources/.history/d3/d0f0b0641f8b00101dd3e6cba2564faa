package gsquestions;

public class JoshephusElimination {

	public static void main(String[] args) {
		int n=7;
		int k=3;
		System.out.println(findTheWinner(n,k));
		
	}

	private static int findTheWinner(int n, int k) {
		if(n<1) return 0;
		int winner=0;
		for(int i=2;i<=n;i++) {
			winner=(winner+k)%i;
		}
		return winner;
	}

}
