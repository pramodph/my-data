package multiThreadingDemos;

class MyThread1 extends Thread{
	public void run() {
		for(int i=0;i<=10;i++) {
			System.out.println("Child Thread");
			Thread.yield();
		}
	}
}
public class Demo03 {
	public static void main(String[] args) {
		MyThread1 t=new MyThread1();
		//t.setPriority(1);
		t.start();
		
		for(int i=0;i<=10;i++) {
			System.out.println("Parent Thread");
		}
	}

}
