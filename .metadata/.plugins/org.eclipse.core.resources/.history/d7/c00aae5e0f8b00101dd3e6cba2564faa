package gsquestions;

public class SortRAndG {

	public static void main(String[] args) {
		char [] arr= {'r','g','r','r','g','r'};
		System.out.println(sortArray(arr));
	}

	private static char[] sortArray(char[] arr) {
		int left=0;
		for(int right=0;right<arr.length;right++) {
			char ch=arr[right];
			if(ch=='r') {
				char temp=arr[left];
				arr[left]=arr[right];
				arr[right]=temp;
				left++;
			}
			
		}
		return arr;
	}

}
