
package gsquestions;

public class FindAverageScoreFrom2D {
	public static void main(String[] args) {
		String[][] details= {
				{"A","84"},
				{"B","84"},
				{"C","84"},
				{"B","87"}
		};
		System.out.println(findHighestAverage(details));
	}

	private static int findHighestAverage(String[][] details) {
			for(int i=0;i<details.length;i++) {
				String name =details[i][0];
				int count=0;
				for(int j=0;j<details.length;j++) {
					if(name.equals(details[j][0])) {
						int sum+=(Integer.parseInt(details[j][1]));
						
					}
				}
			}
	}

}
